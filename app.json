{
  "expo": {
    "name": "Todo Log",
    "slug": "todoLog",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon-Todo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-Todo.png",
      "resizeMode": "contain",
      "backgroundColor": "#C99774"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [],
      "package": "com.todoLog.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-Todo.png",
        "backgroundColor": "#C99774"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
